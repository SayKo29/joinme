{
    "expo": {
        "name": "joinme",
        "slug": "joinme",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/img/logoResized.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/img/logo.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "experiments": {
            "tsconfigPaths": true
        },
        "plugins": [
            [
                "expo-image-picker",
                {
                    "photosPermission": "The app accesses your photos to let you share them with your friends."
                }
            ],
            [
                "expo-location",
                {
                    "locationAlwaysAndWhenInUsePermission": "Esta app necesita que le des permiso para acceder a tu ubicaci√≥n."
                }
            ]
        ],
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "userInterfaceStyle": "automatic",
            "supportsTablet": true,
            "config": {
                "googleMapsApiKey": "AIzaSyCXI6UDD5VVeeDwYwCFY5SKyTCQjbt3OIY"
            },
            "infoPlist": {
                "UIBackgroundModes": ["location", "fetch"]
            },
            "bundleIdentifier": "com.joinme.app"
        },
        "android": {
            "userInterfaceStyle": "automatic",
            "adaptiveIcon": {
                "foregroundImage": "./assets/img/logoResized.png",
                "backgroundColor": "#FFFFFF"
            },
            "softwareKeyboardLayoutMode": "pan",
            "permissions": [
                "ACCESS_FINE_LOCATION",
                "ACCESS_COARSE_LOCATION",
                "ACCESS_BACKGROUND_LOCATION"
            ],
            "config": {
                "googleMaps": {
                    "apiKey": "AIzaSyCXI6UDD5VVeeDwYwCFY5SKyTCQjbt3OIY"
                }
            },
            "package": "com.joinme.app"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "extra": {
            "eas": {
                "projectId": "47e7b089-bf11-4b41-b0f4-6b088e0ead5a"
            }
        }
    }
}
